  "dependencies": {
    "node-sass": "^7.0.0"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "browser-list": "0.0.3",
    "cssnano": "^5.0.14",
    "onchange": "^7.1.0",
    "postcss": "^8.4.5",
